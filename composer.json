{
  "name": "jjrohrer/robustchat",
  "type": "project",
  "description": "Toolkit for learning and utilizing LLM chats, with good-enough UI",
  "homepage": "https://github.com/jjrohrer/RobustChat",
  "keywords": [
    "openai"
  ],
  "authors": [
    {
      "name": "JJ Rohrer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "league/commonmark": "^2.4",
    "orhanerday/open-ai": "^5.0",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "Jjr\\RobustChat\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
    }
  },
  "scripts": {
    "post-autoload-dump": [
    ],
    "post-update-cmd": [
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": { }
  },
  "prefer-stable": true
}
